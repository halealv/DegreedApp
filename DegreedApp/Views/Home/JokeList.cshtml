@model List<APIClient.JokeClass>

@if (Model != null)
{
    <table class="table table-striped table-condensed font-size-sm table-bordered">
        <thead>
            <tr>
                <td>Text</td>
                <td>Word Count</td>
                <td>Length</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Joke</td>
                    <td>@item.Length</td>
                    <td>@item.LengthValue</td>
                </tr>
            }
        </tbody>
    </table>
}
